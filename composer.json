{
  "name": "ytake/hackdotenv",
  "description": "Loads environment variables from .env to getenv()",
  "minimum-stability": "stable",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "hack",
    "hhvm",
    "dotenv",
    "env"
  ],
  "authors": [
    {
      "name": "Yuuki Takezawa",
      "email": "yuuki.takezawa@comnect.jp.net"
    }
  ],
  "require": {
    "hhvm": "^4.20",
    "hhvm/hsl": "^4.15",
    "hhvm/hsl-experimental": "^4.15",
    "hhvm/hhvm-autoload": "^2.0"
  },
  "require-dev": {
    "hhvm/hacktest": "^1.6",
    "facebook/fbexpect": "^2.5.2",
    "hhvm/hhast": "^4.0"
  },
  "scripts": {
    "tests": [
      "hhvm ./vendor/bin/hacktest.hack tests/"
    ],
    "register": [
      "hhvm ./vendor/bin/hh-autoload.hack"
    ]
  }
}
